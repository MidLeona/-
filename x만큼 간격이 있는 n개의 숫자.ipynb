      "source": [
        "def solution(x, n):\n",
        "    answer = []\n",
        "    s = 0\n",
        "    for i in range(n):\n",
        "        s += x\n",
        "        answer.append(s)\n",
        "    return answer\n",
        "\n",
        "print(solution(2,5))\n",
        "print(solution(4,3))\n",
        "print(solution(-4,2))"
      ]
