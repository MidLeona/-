      "source": [
        "def solution(x):\n",
        "    sum = 0\n",
        "    for i in range(len(str(x))):\n",
        "        sum = sum + int(str(x)[i])\n",
        "    if x % sum == 0:\n",
        "        return True\n",
        "    else :\n",
        "        return False\n",
        "\n",
        "print(solution(18))\n",
        "print(solution(25))\n",
        "print(solution(40))"
      ]
    }
